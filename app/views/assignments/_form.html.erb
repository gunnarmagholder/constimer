<% form_for @assignment do |f| %>
  <%= f.error_messages %>
  <p>
	<%= f.hidden_field :manager %>
   <strong><%= f.label :manager, "Manager" %></strong><br />
   <%= text_field_tag 'Manager-Name', @managername, :disabled => true %>	
  </p>
  
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
