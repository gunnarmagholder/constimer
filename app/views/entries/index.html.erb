<h1>Listing entries</h1>

<table>
  <tr>
    <th>User</th>
    <th>Edate</th>
    <th>Starttime</th>
    <th>Endtime</th>
    <th>Follow</th>
    <th>Notes</th>
  </tr>

<% for entry in @entries %>
  <tr>
    <td><%=h entry.user %></td>
    <td><%=h entry.edate %></td>
    <td><%=h entry.starttime %></td>
    <td><%=h entry.endtime %></td>
    <td><%=h entry.follow %></td>
    <td><%=h entry.notes %></td>
    <td><%= link_to 'Show', entry %></td>
    <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
    <td><%= link_to 'Destroy', entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New entry', new_entry_path %>
